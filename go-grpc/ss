/admin/v1/crontab/create_month_table


CREATE TABLE `t_record201802` (
  `id` bigint(40) NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `company_id` smallint(6) NOT NULL COMMENT '公司ID',
  `user_id` int(10) NOT NULL COMMENT '用户ID',
  `sign_avatar` varchar(255) NOT NULL DEFAULT '' COMMENT '头像',
  `sign_bg_avatar` varchar(255) NOT NULL COMMENT '完整照片',
  `sign_time` varchar(11) NOT NULL DEFAULT '' COMMENT '打卡时间',
  `sign_date` varchar(20) NOT NULL DEFAULT '' COMMENT '按天记时',
  `source` enum('1','0') NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  KEY `date_index` (`sign_date`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=282 DEFAULT CHARSET=utf8 COMMENT='打卡记录表';


