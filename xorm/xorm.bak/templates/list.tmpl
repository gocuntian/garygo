<html>
    <head>
        <title>list</title>
    </head>
    <body>
        <table class="table table-striped table-bordered">
				<thead>
					<tr>
						<th>ID</th>
						<th>Name</th>
					</tr>
				</thead>
				<tbody>
				{{range $k, $v := .List}}
					<tr>
						<td>{{$v.Id}}</td>
						<td>{{$v.Name}}</td>
					</tr>
				{{end}}
				</tbody>
				</table>
    </body>
</html>